h4 Accounting

.container-fluid
  .row
    .form-inline
      metric-selector(label="Metric:")
      granularity-selector(label="Granularity:")

  .row
    daterange-selector

  aggregate
    .row
      .card
        .card-block(style="padding: 0;")
          aggregate-graph

        .card-footer
          aggregate-download

    .row
      .card
        .card-header
          h5 Totals from {{ daterange?.start | date: 'y-MM-dd HH:mm Z' }} to {{ daterange?.end | date: 'y-MM-dd HH:mm Z' }}

        .card-block
          aggregate-table
